	depends on \
		( ! FREETZ_TYPE_LABOR || \
		FREETZ_TYPE_LABOR_AIO && \
		(FREETZ_TYPE_FON_WLAN_7170 || FREETZ_TYPE_FON_WLAN_7270) ) && \
		! (FREETZ_TYPE_FON && FREETZ_TYPE_LANG_EN)
