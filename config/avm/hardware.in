# cpu model (the same variable name as in /proc/cpuinfo)

config FREETZ_CPU_MODEL_MIPS_4KEc
	bool
	default y if \
		FREETZ_SYSTEM_TYPE_AR7_SANGAM || \
		FREETZ_SYSTEM_TYPE_AR7_OHIO   || \
		FREETZ_SYSTEM_TYPE_UR8

config FREETZ_CPU_MODEL_MIPS_24KEc
	bool
	default y if \
		FREETZ_SYSTEM_TYPE_IKS

config FREETZ_CPU_MODEL_MIPS_34Kc
	bool
	default y if \
		FREETZ_SYSTEM_TYPE_AR9  || \
		FREETZ_SYSTEM_TYPE_AR10 || \
		FREETZ_SYSTEM_TYPE_VR9

config FREETZ_CPU_MODEL_MIPS_74Kc
	bool
	default y if \
		FREETZ_SYSTEM_TYPE_AR934x  || \
		FREETZ_SYSTEM_TYPE_QCA955x || \
		FREETZ_SYSTEM_TYPE_QCA956x

config FREETZ_CPU_MODEL_MIPS_interAptiv
	bool
	default y if \
		FREETZ_SYSTEM_TYPE_GRX5

config FREETZ_CPU_MODEL_X86_ATOM
	bool
	default y if \
		FREETZ_SYSTEM_TYPE_PUMA6_X86
