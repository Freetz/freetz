config FREETZ_SYSTEM_TYPE_GRX5
	bool
	default y if \
		FREETZ_TYPE_6890 || \
		FREETZ_TYPE_7560 || \
		FREETZ_TYPE_7580 || \
		FREETZ_TYPE_7590

config FREETZ_SYSTEM_TYPE_IKS_VX180
	bool
	default y if \
		FREETZ_TYPE_7340 || \
		FREETZ_TYPE_7390

config FREETZ_SYSTEM_TYPE_IKS_VX185
	bool
	default y if \
		FREETZ_TYPE_7369

config FREETZ_SYSTEM_TYPE_IKS
	bool
	default y
	depends on \
		FREETZ_SYSTEM_TYPE_IKS_VX180 || \
