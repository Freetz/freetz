	# no support for 06.0x
	select FREETZ_AVM_SOURCE_6810_06_21 if FREETZ_AVM_VERSION_06_2X
	select FREETZ_AVM_SOURCE_6810_06_21 if FREETZ_AVM_VERSION_06_5X
