config FREETZ_GCC_ABI
	string
	default "32"

config FREETZ_GCC_ARCH
	string
	default "34kc"     if FREETZ_TARGET_ARCH_MIPS && FREETZ_TARGET_ARCH_BE && ( FREETZ_KERNEL_LAYOUT_VR9 || FREETZ_KERNEL_LAYOUT_GRX5 )
	default "mips32"   if FREETZ_TARGET_ARCH_MIPS && FREETZ_TARGET_ARCH_LE
	default "mips32r2" if FREETZ_TARGET_ARCH_MIPS && FREETZ_TARGET_ARCH_BE

config FREETZ_GCC_TUNE
	string
	default "34kc"     if FREETZ_TARGET_ARCH_MIPS && FREETZ_TARGET_ARCH_BE && ( FREETZ_KERNEL_LAYOUT_VR9 || FREETZ_KERNEL_LAYOUT_GRX5 )
	default "4kc"      if FREETZ_TARGET_ARCH_MIPS && FREETZ_TARGET_ARCH_LE
	default "24kc"     if FREETZ_TARGET_ARCH_MIPS && FREETZ_TARGET_ARCH_BE

config FREETZ_GCC_FLOAT_ABI
	string
	default "soft"
