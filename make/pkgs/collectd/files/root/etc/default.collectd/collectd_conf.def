CAPTION='collectd.conf (Collectd)'
DESCRIPTION="$(lang de:"Konfigurationsdatei f&uuml;r den Collectd Daemon. <a href=\"http://collectd.org/documentation/manpages/collectd.conf.5.shtml\">Collectd Dokumentation</a>." en:"Config file for the Collectd Daemon. <a href=\"http://collectd.org/documentation/manpages/collectd.conf.5.shtml\">Collectd documenation</a>.")"

CONFIG_FILE='/tmp/flash/collectd/collectd.conf'
CONFIG_SAVE='modsave flash; if [ ! -z "$(pidof collectd)" ]; then echo ""; /mod/etc/init.d/rc.collectd restart; fi'
CONFIG_TYPE='text'

