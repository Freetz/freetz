ifneq ($(strip $(FREETZ_SYSTEM_TYPE_BCM63138)),y)
TARGET_CFLAGS_HW_CAPABILITIES += $(if $(call qstrip,$(FREETZ_GCC_FPU)),-mfpu=$(call qstrip,$(FREETZ_GCC_FPU)))
