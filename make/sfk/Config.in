config FREETZ_PACKAGE_SFK
	bool "Swiss File Knife 1.7.6"
	default n
	select FREETZ_LIB_STDCXXLIB
	select FREETZ_LIB_libpthread if !FREETZ_PACKAGE_SFK_STATIC
	help
		build the freeware version of sfk, see the site
		http://stahlworks.com/dev/swiss-file-knife.html
		for further information

config FREETZ_PACKAGE_SFK_STATIC
	bool "link statically"
	default n
	depends on FREETZ_PACKAGE_SFK
	help
		create a version usable without additional DSOs
