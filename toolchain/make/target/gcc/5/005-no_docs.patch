--- gcc/Makefile.in
+++ gcc/Makefile.in
@@ -2878,7 +2878,8 @@
 
 # Remake the info files.
 
-doc: $(BUILD_INFO) $(GENERATED_MANPAGES)
+doc:
+doc-orig: $(BUILD_INFO) $(GENERATED_MANPAGES)
 
